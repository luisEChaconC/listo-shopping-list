{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc && npx tsc-alias",
    "start": "node dist/main.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev --prefix src/client\"",
    "dev:backend": "nodemon --exec ts-node -r tsconfig-paths/register src/backend/main.ts",
    "test": "npx jest",
    "lint": "npx eslint . --ext .js,.ts",
    "format": "prettier --write ."
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/de-mi-tierra/web-site/backend.git"
  },
