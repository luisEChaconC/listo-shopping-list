  },
  "homepage": "https://gitlab.com/de-mi-tierra/web-site/backend#readme",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@swc/jest": "^0.2.39",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.14",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "concurrently": "^8.2.2",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
